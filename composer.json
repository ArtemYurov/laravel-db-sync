{
    "name": "artemyurov/laravel-db-sync",
    "description": "Laravel package for synchronizing databases from remote servers via SSH tunnel",
    "keywords": [
        "laravel",
        "database",
        "sync",
        "postgresql",
        "ssh-tunnel",
        "pg_dump",
        "backup",
        "restore"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Yurov",
            "email": "artem@yurov.org"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/config": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "artemyurov/laravel-autossh-tunnel": "^0.5.0",
        "symfony/process": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "ArtemYurov\\DbSync\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArtemYurov\\DbSync\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArtemYurov\\DbSync\\DbSyncServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
